<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.TextArea?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:id="fileListPanel" fx:controller="FileListController"
            maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="480.0" prefWidth="720.0"
            stylesheets="stylesheet.css">
    <children>
        <TextField fx:id="location" onKeyReleased="#onLocationKeyReleased" editable="true"
                   layoutX="2.0" layoutY="2.0" prefHeight="25.0" prefWidth="716.0"/>
        <ListView fx:id="list" onKeyReleased="#onListKeyReleased" editable="false"
                  layoutX="2.0" layoutY="56.0" prefHeight="422.0" prefWidth="716.0"/>
        <TextArea fx:id="viewer" onKeyReleased="#onViewerKeyReleased" editable="false" wrapText="true"
                  layoutX="2.0" layoutY="56.0" prefHeight="422.0" prefWidth="716.0" visible="false"/>
    </children>
</AnchorPane>
